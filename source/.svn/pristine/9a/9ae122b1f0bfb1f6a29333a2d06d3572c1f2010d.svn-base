#include "ChargeResolution.h"
#include <Run.h>

bool ChargeResolution::mStaticSetup = false;
map<int,double> ChargeResolution::mRequirementMap;
map<int,double> ChargeResolution::mGoalMap;
map<int,double> ChargeResolution::mScaleMap;
int ChargeResolution::mScaleMax = 0;
TGraph* ChargeResolution::mRequirementGraph;
TGraph* ChargeResolution::mRequirementScaledGraph;
TGraph* ChargeResolution::mGoalGraph;
TGraph* ChargeResolution::mGoalScaledGraph;
TGraph* ChargeResolution::mTestGraph;

void ChargeResolution::Init() {
	mSumMap.clear();
	mNMap.clear();
	mHistMap_heide.clear();
	if (mMaxNPE > pRun->GetBranchMax(mTrueBranch)) mMaxNPE = (int)pRun->GetBranchMax(mTrueBranch);
	mScoreTotal = 0;
	mScoreLow = 0;
	mScoreHigh = 0;
}

void ChargeResolution::FillCharges() {
	COUTP("[chargeres] Extracting entry information");
	mVariationHist = new TH2F("","",log10(mMaxNPE)*50,log10(1),log10(mMaxNPE),log10(mMaxNPE)*50,log10(1),log10(mMaxNPE));
	mVariationHist->SetStats(0);
	char *hname = new char[10];
//	for (int i = 0; i <= mMaxNPE; i++) {
//		mHistMap_heide[i] = new TH1F(hname, hname, 1000, -10, 1180);
//	}
	Progress progress(pRun->GetNumEventTTreeEntries());
	int i = 0;
	for (auto& camera_pair : pRun->GetCameraMap()) {
		auto& camera = camera_pair.second;
		for (auto& event_pair : camera.GetEventMap()) {
			auto& event = event_pair.second;
			for (auto& entry_pair : event.GetEntryMap()) {
				auto& entry = entry_pair.second;
				progress.Print(i++);
				int trueQ = (int) entry.GetBranchValue(mTrueBranch);
				float measuredQ = entry.GetBranchValue(mMeasuredBranch);
				if (trueQ <= mMaxNPE) {
					mSumMap[trueQ] += pow(measuredQ - trueQ, 2);
					mNMap[trueQ]++;
//					mHistMap_heide[trueQ]->Fill(measuredQ);
					mVariationHist->Fill(log10(trueQ),log10(measuredQ));
				}
			}
		}
	}
//	mVariationHist->GetYaxis()->SetRange(min,max);
}

void ChargeResolution::SetChargeResolution() {
	mChargeResolutionGraph = new TGraphErrors();
	mChargeResolutionScaledGraph = new TGraphErrors();
	for (int trueQ = 1; trueQ <= mMaxNPE; trueQ++) {
		if (mNMap[trueQ] > 0) {
//			double chargeres_error = (1/trueQ)*(1/sqrt(mNMap[trueQ]));
//			if (mHistMap_heide[trueQ]->GetMean() != 0) {
//				chargeres_error = (float) mHistMap_heide[trueQ]->GetRMSError() /
//				                          (float) mHistMap_heide[trueQ]->GetMean();
//			}


//			double sum = 0.;
//			for (int j = 0; j <= mHistMap_heide[trueQ]->GetNbinsX(); j++) {
//				double measuredQ = mHistMap_heide[trueQ]->GetBinCenter(j);
//				double NmeasuredQ = mHistMap_heide[trueQ]->GetBinContent(j);
//				if (NmeasuredQ > 0) {
//					sum += pow((measuredQ - trueQ), 2) * NmeasuredQ;
//				}
//			}
//			double chargeres = (float) sqrt((sum / mNMap[trueQ]) + trueQ) / trueQ;  // heide

			double chargeres = sqrt((mSumMap[trueQ] / mNMap[trueQ])) / trueQ;

			int current_point = mChargeResolutionGraph->GetN();
			mChargeResolutionGraph->SetPoint(current_point, trueQ, chargeres);
//			mChargeResolutionGraph->SetPointError(current_point, 0, chargeres_error);
			if (trueQ <= mScaleMax) {
				mChargeResolutionScaledGraph->SetPoint(current_point, trueQ, chargeres / mScaleMap[trueQ]);
//				mChargeResolutionScaledGraph->SetPointError(current_point, 0, chargeres_error / mScaleMap[trueQ]);
			}

			// Scoring
			mScoreTotal += chargeres;
			if (trueQ <= 10) {
				mScoreLow += chargeres;
			}
			else {
				mScoreHigh += chargeres;
			}
		}
	}
}

